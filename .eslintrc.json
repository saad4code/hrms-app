{
    "env": {
        "browser": true,
        "es2020": false
    },
    "extends": [
        "eslint:recommended",
        "plugin:react/recommended"
    ],
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "no-unused-vars": "error",
        "no-tabs": [
            "error",
            {
                "allowIndentationTabs": true
            }
        ],
        "react/prop-types": "off",
        "no-console": "warn",
        "no-undef": "off",
        "func-names": "off",
        "camelcase": "error"
    }
}